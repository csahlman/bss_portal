<div class= "depot_form">
  <%= form_tag do %>
    <fieldset>
      <legend>Request a new login</legend>
      <div>
      <%= label_tag :email, 'Email:' %>
      <%= text_field_tag :email, params[:email] %>
      </div>
      <div>
      <%= submit_tag "Login" %>
      </div>
    </fieldset>

  <% end %>
</div>
